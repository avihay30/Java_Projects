               <padding>
                  <Insets top="5.0" />
               </padding>
            </HBox>
         </children>
         <padding>
            <Insets right="10.0" />
         </padding>
      </VBox>
      <StackPane fx:id="stackPane" prefHeight="282.0" prefWidth="291.0" />
   </children>
</HBox>
